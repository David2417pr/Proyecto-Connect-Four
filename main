
#include <iostream>
using namespace  std;

int main()
{
    
    for (int number = 10; number > -10; number -=2 )
        cout << number << ",";
    
    for (int number = 1; number <= 22; number+= 3)
        if ( number == 22)
        cout << number;
        else
        cout << number << ",";
    
        

    return 0;
}
